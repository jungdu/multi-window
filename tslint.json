{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js"]
  },
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "noUnusedLocals": true,
    "ordered-imports": false
  },
  "jsRules": {
    "no-console": false,
    "object-literal-sort-keys": false
  }
}
